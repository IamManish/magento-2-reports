<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Backend\Model\Url">
        <plugin name="startup_page_url" type="Mageplaza\Reports\Plugin\Model\StartupPageUrl" sortOrder="1"/>
    </type>
    <type name="Magento\Backend\Helper\Data">
        <plugin name="backend_logo_url" type="Mageplaza\Reports\Plugin\BackendLogoUrl" sortOrder="1"/>
    </type>
    <type name="Mageplaza\Reports\Model\CardsManageFactory">
        <arguments>
            <argument name="map" xsi:type="array">
                <item name="lastOrders" xsi:type="string">Mageplaza\Reports\Block\Dashboard\LastOrders</item>
                <item name="lastSearches" xsi:type="string">Mageplaza\Reports\Block\Dashboard\LastSearches</item>
                <item name="topSearches" xsi:type="string">Mageplaza\Reports\Block\Dashboard\TopSearches</item>
                <item name="orders" xsi:type="string">Mageplaza\Reports\Block\Dashboard\Orders</item>
                <item name="averageOrderValue" xsi:type="string">Mageplaza\Reports\Block\Dashboard\AverageOrderValue</item>
                <item name="salesByLocation" xsi:type="string">Mageplaza\Reports\Block\Dashboard\SalesByLocation</item>
                <item name="repeatCustomerRate" xsi:type="string">Mageplaza\Reports\Block\Dashboard\RepeatCustomerRate</item>
                <item name="totalSales" xsi:type="string">Mageplaza\Reports\Block\Dashboard\TotalSales</item>
                <item name="bestsellers" xsi:type="string">Mageplaza\Reports\Block\Dashboard\Bestsellers</item>
                <item name="mostViewedProducts" xsi:type="string">Mageplaza\Reports\Block\Dashboard\MostViewedProducts</item>
                <item name="newCustomers" xsi:type="string">Mageplaza\Reports\Block\Dashboard\NewCustomers</item>
                <item name="customers" xsi:type="string">Mageplaza\Reports\Block\Dashboard\Customers</item>
                <item name="tax" xsi:type="string">Mageplaza\Reports\Block\Dashboard\Tax</item>
                <item name="shipping" xsi:type="string">Mageplaza\Reports\Block\Dashboard\Shipping</item>
                <item name="lifetimeSales" xsi:type="string">Mageplaza\Reports\Block\Dashboard\LifetimeSales</item>
                <item name="averageOrder" xsi:type="string">Mageplaza\Reports\Block\Dashboard\AverageOrder</item>
            </argument>
        </arguments>
    </type>
    <type name="Mageplaza\Reports\Block\Menu">
        <arguments>
            <argument name="girdName" xsi:type="array">
                <item name="mpreports_details_salesbycustomergroup" xsi:type="string">mp_reports_sales_by_customer_group_grid.mp_reports_sales_by_customer_group_details_listing_data_source</item>
                <item name="mpreports_details_abandonedcarts" xsi:type="string">mp_reports_abandoned_carts_grid.mp_reports_abandoned_carts_details_listing_data_source</item>
                <item name="mpreports_details_stockvssold" xsi:type="string">mp_reports_stock_vs_sold_grid.mp_reports_stock_vs_sold_details_listing_data_source</item>
                <item name="mpreports_details_bestsellers" xsi:type="string">mp_reports_bestsellers_grid.mp_reports_bestsellers_details_listing_data_source</item>
                <item name="mpreports_details_salesbyproduct" xsi:type="string">mp_reports_sales_by_product_grid.mp_reports_sales_by_product_details_listing_data_source</item>
                <item name="mpreports_details_salesbylocation" xsi:type="string">mp_reports_sales_by_location_grid.mp_reports_sales_by_location_details_listing_data_source</item>
                <item name="mpreports_details_salesbystate" xsi:type="string">mp_reports_sales_by_state_grid.mp_reports_sales_by_location_details_listing_data_source</item>
                <item name="mpreports_details_salesbycity" xsi:type="string">mp_reports_sales_by_city_grid.mp_reports_sales_by_location_details_listing_data_source</item>
                <item name="mpreports_details_salesbycustomer" xsi:type="string">mp_reports_sales_by_customer_grid.mp_reports_sales_by_customer_details_listing_data_source</item>
            </argument>
            <argument name="menuUrls" xsi:type="array">
                <item name="mpreports/details/salesbycustomergroup" xsi:type="string" translate="true">Sales By Customer Group</item>
                <item name="mpreports/details/abandonedcarts" xsi:type="string" translate="true">Abandoned Carts</item>
                <item name="mpreports/details/stockvssold" xsi:type="string" translate="true">Stock vs Sold</item>
                <item name="mpreports/details/bestsellers" xsi:type="string" translate="true">Bestseller</item>
                <item name="mpreports/details/salesbyproduct" xsi:type="string" translate="true">Sales By Product</item>
                <item name="mpreports/details/salesbylocation" xsi:type="string" translate="true">Sales By Location</item>
                <item name="mpreports/details/salesbycustomer" xsi:type="string" translate="true">Sales By Customer</item>
            </argument>
        </arguments>
    </type>
</config>
